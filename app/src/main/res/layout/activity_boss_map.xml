<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:id="@+id/activity_boss_map"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="ucsd.cse110fa16.group14.ipark.BossMap">


    <Button
        android:layout_height="wrap_content"
        android:id="@+id/help"
        android:background="@mipmap/help"
        tools:ignore="HardcodedText"
        android:layout_width="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        tools:ignore="HardcodedText"
        android:id="@+id/status"
        android:background="@android:drawable/sym_def_app_icon"
        android:layout_alignParentTop="false"
        android:layout_alignParentStart="false" />

    <ucsd.cse110fa16.group14.ipark.MapInteractive
        android:layout_marginTop="25dp"
        android:layout_width="match_parent"
        android:layout_height="380dp"
        android:layout_below="@+id/help"
        android:layout_centerHorizontal="true" />


</RelativeLayout>