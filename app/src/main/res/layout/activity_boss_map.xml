<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:id="@+id/activity_boss_map"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="ucsd.cse110fa16.group14.ipark.BossMap">


    <Button
        android:layout_height="wrap_content"
        android:id="@+id/help"
        android:background="@mipmap/help"
        tools:ignore="HardcodedText"
        android:layout_width="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <ucsd.cse110fa16.group14.ipark.MapInteractive
        android:layout_width="match_parent"
        android:layout_height="380dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView60"
        android:textSize="26sp"
        android:textColor="#222222"
        android:layout_marginStart="15dp"
        android:textAlignment="center"
        android:layout_below="@+id/status" />

    <Button
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        tools:ignore="HardcodedText"
        android:id="@+id/status"
        android:background="@android:drawable/sym_def_app_icon"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView60"
        android:layout_marginStart="11dp" />


</RelativeLayout>