        android:textSize="20dp"
        android:layout_alignStart="@+id/textView46"
        android:layout_marginTop="25dp"
        android:id="@+id/textView47" />

    <TextView
        android:text="Username: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView47"
        android:layout_alignStart="@+id/textView47"
        android:layout_marginTop="29dp"
        android:textStyle="bold"
        android:textSize="20dp"
        android:id="@+id/textView48" />

    <TextView
        android:text="Password:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView48"
        android:layout_alignStart="@+id/textView48"
        android:layout_marginTop="33dp"
        android:textStyle="bold"
        android:textSize="20dp"
        android:id="@+id/textView49" />

    <TextView
        android:text="Meow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView51"
        android:textSize="19dp"
        android:layout_alignTop="@+id/textView45"
        android:layout_alignStart="@+id/textView50" />

    <TextView
        android:text="hellokitty@world.com"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView52"
        android:textSize="19dp"
        android:layout_above="@+id/textView47"
        android:layout_alignStart="@+id/textView51" />

    <TextView
        android:text="helloKittyRocks123"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="19dp"
        android:layout_alignBottom="@+id/textView48"
        android:layout_alignStart="@+id/textView52"
        android:id="@+id/textView54" />

    <TextView
        android:text="************"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="19dp"
        android:layout_alignBottom="@+id/textView49"
        android:layout_toEndOf="@+id/textView47"
        android:id="@+id/textView55" />

    <TextView
        android:text="Hello Kitty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView50"
        android:textSize="19dp"
        android:layout_alignBaseline="@+id/textView44"
        android:layout_alignBottom="@+id/textView44"
        android:layout_toEndOf="@+id/textView46" />

    <TextView
        android:text="D1234566"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:id="@+id/textView53"
        android:layout_alignBottom="@+id/textView47"
        android:layout_alignEnd="@+id/textView55" />

    <Button
        android:text="UPDATE INFO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="55dp"
        android:id="@+id/button19"
        android:layout_below="@+id/textView49"
        android:layout_toStartOf="@+id/textView54"
        android:layout_marginEnd="10dp" />

    <Button
        android:text="CANCEL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button19"
        android:layout_alignStart="@+id/textView55"
        android:id="@+id/button21" />

</RelativeLayout>