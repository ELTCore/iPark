<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_settings"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#ffffff"
    tools:context="ucsd.cse110fa16.group14.ipark.UserHomepage">


    <TextView
        android:text="History"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="6"
        android:textSize="15dp"
        android:textStyle="bold"
        android:id="@+id/textView39"
        android:layout_below="@+id/reviewHistory"
        android:layout_alignEnd="@+id/reviewHistory"
        android:textColor="#404040"
        android:textAlignment="center" />

    <TextView
        android:text="Status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="6"
        android:textSize="15dp"
        android:textStyle="bold"
        android:id="@+id/textView38"
        android:layout_below="@+id/checkStatus"
        android:layout_alignParentEnd="true"
        android:textAlignment="center"
        android:textColor="#404040" />

    <ImageButton
        app:srcCompat="@mipmap/logout_button"
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:layout_width="110dp"
        tools:background="@android:color/background_light"
        android:layout_height="120dp"
        android:scaleType="fitCenter"
        android:id="@+id/logoutButton"
        android:layout_above="@+id/textView43"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="Logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="6"
        android:textSize="15dp"
        android:textStyle="bold"
        android:id="@+id/textView42"
        android:layout_below="@+id/emergency"
        android:layout_alignParentEnd="true"
        android:textAlignment="center"
        android:textColor="#404040" />

    <TextView
        android:text="Map/Report"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="6"
        android:textSize="15dp"
        android:textStyle="bold"
        android:id="@+id/textView41"
        android:layout_below="@+id/viewMap"
        android:layout_alignEnd="@+id/viewMap"
        android:textAlignment="center"
        android:textColor="#202020" />

    <ImageButton
        app:srcCompat="@mipmap/check_status"
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:layout_width="110dp"
        tools:background="@android:color/background_light"
        android:layout_height="120dp"
        android:scaleType="fitCenter"
        android:id="@+id/checkStatus"
        android:layout_alignTop="@+id/viewMap"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:layout_width="110dp"
        tools:background="@android:color/background_light"
        android:layout_height="120dp"
        android:scaleType="fitCenter"
        app:srcCompat="@mipmap/emergency_button"
        android:id="@+id/emergency"
        android:layout_below="@+id/textView39" />

    <TextView
        android:text="Emergency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="6"
        android:textSize="15dp"
        android:textStyle="bold"
        android:id="@+id/textView43"
        android:layout_below="@+id/emergency"
        android:layout_alignEnd="@+id/emergency"
        android:textAlignment="center"
        android:textColor="@color/colorAccent"
        tools:ignore="HardcodedText" />

    <ImageButton
        app:srcCompat="@mipmap/map_button"
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:layout_width="110dp"
        tools:background="@android:color/background_light"
        android:layout_height="120dp"
        android:scaleType="fitCenter"
        android:id="@+id/viewMap"
        android:layout_alignTop="@+id/reviewHistory"
        android:layout_alignEnd="@+id/textView37" />

    <ImageButton
        app:srcCompat="@mipmap/personal_info"
        android:id="@+id/personalInfo"
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:layout_width="110dp"
        tools:background="@android:color/background_light"
        android:layout_height="120dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        app:srcCompat="@mipmap/reserve_button"
        android:id="@+id/reserveButton"
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:layout_width="110dp"
        tools:background="@android:color/background_light"
        android:layout_height="120dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:ems="6"
        android:textSize="15dp"
        android:textStyle="bold"
        android:id="@+id/text2"
        android:textAlignment="center"
        android:textColor="#404040"
        android:layout_below="@+id/personalInfo"
        android:layout_alignEnd="@+id/personalInfo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reserve"
        android:ems="6"
        android:textSize="15dp"
        android:textStyle="bold"
        android:id="@+id/textView37"
        android:textAlignment="center"
        android:textColor="#000000"
        android:layout_below="@+id/reserveButton"
        android:layout_alignStart="@+id/reserveButton" />

    <ImageButton
        app:srcCompat="@mipmap/review_history"
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:layout_width="110dp"
        tools:background="@android:color/background_light"
        android:layout_height="120dp"
        android:scaleType="fitCenter"
        android:id="@+id/reviewHistory"
        android:layout_below="@+id/text2"
        android:layout_alignParentStart="true" />


</RelativeLayout>