<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_review"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="ucsd.cse110fa16.group14.ipark.activity_review"
    android:layout_alignParentLeft="true">


    <TextView
        android:text="19:00"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_below="@+id/textView2"
        android:id="@+id/textView8"
        android:textAlignment="viewStart"
        android:textSize="25sp"
        android:layout_toRightOf="@+id/textView4"
        android:paddingLeft="17dp"
        android:textColor="#000000" />

    <TextView
        android:text="CSE110 PARKING COMPANY"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:id="@+id/textView"
        android:textAlignment="center"
        tools:textColor="#708090"
        android:textSize="18sp" />

    <Button
        android:text="nop"
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:id="@+id/button3"
        android:textAlignment="center"
        android:textSize="18sp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/button"
        android:layout_alignParentRight="false"
        android:layout_alignParentLeft="false" />

    <Button
        android:text="COMMENT?"
        android:id="@+id/button"
        android:layout_height="75dp"
        android:textSize="18sp"
        android:layout_width="175dp"
        android:textAlignment="center"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/button3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="false"
        android:paddingLeft="0dp"
        android:paddingRight="0dp" />

    <TextView
        android:text="START TIME: "
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/textView4"
        android:textSize="24sp"
        android:textAlignment="textStart"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true"
        android:textColor="#000000"
        android:paddingLeft="16dp"
        android:layout_alignEnd="@+id/textView3" />

    <TextView
        android:text="$10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_marginTop="29dp"
        tools:layout_height="70dp"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="50sp"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="END TIME: "
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/textView3"
        android:textSize="24sp"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:paddingLeft="16dp"
        android:layout_toStartOf="@+id/button3" />

    <TextView
        android:text="PRICE PER HOUR"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/textView11"
        android:paddingLeft="16dp"
        android:textColor="#696969"
        android:textAlignment="center"
        android:textSize="21sp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button3"
        android:layout_centerHorizontal="true"
        android:id="@+id/ratingBar" />

    <TextView
        android:text="$2.5/hr"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:id="@+id/textView12"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="27sp"
        android:layout_above="@+id/ratingBar"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="10dp" />

    <TextView
        android:text="RATE YOUR PARKING"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:id="@+id/textView5"
        android:textSize="18sp"
        android:textAlignment="center"
        android:textColor="#696969"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignParentStart="true" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:id="@+id/view1"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp" />

    <View
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:id="@+id/view2"
        android:textAlignment="center"
        android:layout_marginStart="180dp"
        android:layout_width="150dp"
        android:layout_below="@+id/textView8"
        android:layout_marginTop="10dp"/>

    <View
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:id="@+id/view3"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="10dp"
        android:textAlignment="center"
        android:layout_marginStart="180dp"
        android:layout_width="150dp" />



    <TextView
        android:layout_width="200dp"
        android:id="@+id/textView10"
        android:textSize="25sp"
        android:textColor="#000000"
        android:textAlignment="viewStart"
        android:layout_height="25dp"
        android:layout_below="@+id/textView4"

        android:text="23:00"
        android:paddingLeft="17dp"
        android:layout_toEndOf="@+id/textView3" />

</RelativeLayout>
