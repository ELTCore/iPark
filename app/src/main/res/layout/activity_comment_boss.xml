        android:paddingLeft="10dp"
        android:backgroundTint="#CCE5FF"
        android:paddingBottom="5dp"
        android:paddingTop="5dp"
        android:layout_below="@+id/textView28" />

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/button15"
        android:layout_above="@+id/textView30">



        <RadioButton
            android:text="Date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton"
            android:layout_alignBaseline="@+id/button15"
            android:layout_alignBottom="@+id/button15"
            android:layout_toEndOf="@+id/button15"
            android:checked="true"
            android:layout_marginTop="5dp" />
        <RadioButton
            android:text="Length"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/radioButton"
            android:layout_toEndOf="@+id/radioButton"
            android:id="@+id/radioButton2"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp" />
    </RadioGroup>

    <Button
        android:text="Sorted by"
        android:layout_width="wrap_content"
        android:id="@+id/button15"
        android:textAllCaps="false"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="previous"
        android:layout_height="wrap_content"
        android:id="@+id/submitButton"
        android:layout_width="100dp"
        android:textAlignment="center"
        android:textSize="18sp"
        tools:ignore="HardcodedText"
        android:textAllCaps="false"
        android:layout_below="@+id/textView35"
        android:layout_alignStart="@+id/textView35"
        android:layout_marginStart="40dp" />

    <Button
        android:text="next "
        android:layout_width="100dp"
        android:layout_height="wrap_content"

        android:id="@+id/cancelButton"
        android:textSize="18sp"
        android:textAlignment="center"
        tools:ignore="HardcodedText"
        android:textAllCaps="false"
        android:layout_below="@+id/textView35"
        android:layout_alignEnd="@+id/textView35"
        android:layout_marginEnd="40dp" />
