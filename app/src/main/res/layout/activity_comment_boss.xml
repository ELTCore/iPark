        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:textSize="16sp"
        android:textColor="#404040"
        android:paddingLeft="10dp"
        android:backgroundTint="#CCE5FF"
        android:paddingBottom="5dp"
        android:paddingTop="5dp"
        android:layout_below="@+id/textView36"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp" />

    <TextView
        android:text="hard to figure my parking place obviously need to improve the map direction feature"
        android:layout_width="match_parent"
        android:background="@drawable/lighter_border"
        android:layout_marginTop="10dp"
        android:id="@+id/textView32"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
